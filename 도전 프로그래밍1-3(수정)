#define _CRT_SECURE_NO_WARNINGS
 #include <stdio.h>
 #include <stdlib.h>
 #include <string.h>
 #include <math.h>
 #include <time.h>

void GCD(int c, int d)
{
	int i;
	int minnum;
	int div;

	minnum = min(c, d);


	for (i = 1; i <= minnum; i++)
	{
		if (c % i == 0 && d % i == 0)
			div = i;
						// 둘 다 모듈러 연산 수행을 해서 약수가 나온 경우
	}
	printf("%d\n", div);
}

int main(void)
{
	int a, b;

	printf("두 개의 정수 입력 : ");
	scanf("%d %d", &a, &b);

	printf("두 수의 최대공약수 : ");
	GCD(a, b);
	

	return 0;
}
